{
  "name": "element-lite-starter-kit",
  "version": "0.0.2",
  "description": "Element Lite Starter Kit",
  "main": "index.js",
  "scripts": {
    "postinstall": "./delete.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm-run-all": "./node_modules/.bin/npm-run-all --parallel",
    "compile": "node compiler",
    "compile-watch": "node compiler --dev --watch",
    "superstatic-dev": "./node_modules/.bin/superstatic -c superstatic.json public",
    "superstatic-dev-c9": "./node_modules/.bin/superstatic -c superstatic.json public --port $PORT --host $IP",
    "webpack-dev": "./node_modules/.bin/webpack --env.BROWSERS=module --mode=development --watch",
    "webpack-prod": "./node_modules/.bin/webpack --mode=production",
    "webpack-prod-module": "./node_modules/.bin/webpack --mode=production --env.BROWSERS=module",
    "start-c9": "npm run npm-run-all --parallel compile-watch superstatic-dev-c9 webpack-dev",
    "serve": "node server",
    "start": "npm run npm-run-all compile-watch superstatic-dev webpack-dev",
    "workbox": "./node_modules/.bin/workbox generateSW workbox.config.js",
    "prod": "NODE_ENV=production && rm -r public && ./node_modules/.bin/npm-run-all compile webpack-prod webpack-prod-module compile workbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmonsi/element-lite-starter-kit.git"
  },
  "author": "TJ Monserrat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmonsi/element-lite-starter-kit/issues"
  },
  "homepage": "https://github.com/tjmonsi/element-lite-starter-kit#readme",
  "dependencies": {
    "@littleq/core-lite": "0.0.5",
    "@littleq/element-lite": "^0.3.6",
    "@webcomponents/shadycss": "^1.5.0",
    "@webcomponents/webcomponentsjs": "2.1.1",
    "es5-shim": "^4.5.10",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "intersection-observer": "^0.5.0",
    "lit-html": "^0.11.0",
    "localforage": "^1.7.1",
    "object-fit-images": "^3.2.3",
    "raven-js": "^3.26.2",
    "weakmap-polyfill": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "flat": true,
  "sideEffects": false,
  "size-limit": [],
  "semistandard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.17",
    "connect": "3.6.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "0.28.9",
    "ejs": "2.5.7",
    "eslint": "4.16.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-plugin-html": "4.0.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.0.1",
    "file-loader": "1.1.6",
    "globby": "7.1.1",
    "gzip-size-cli": "2.1.0",
    "html-minifier": "3.5.8",
    "html-webpack-plugin": "3.2.0",
    "is-glob": "4.0.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "2.0.10",
    "preload-webpack-plugin": "2.3.0",
    "raw-loader": "0.5.1",
    "semistandard": "11.0.0",
    "size-limit": "0.18.1",
    "style-loader": "0.20.1",
    "stylus": "0.54.5",
    "stylus-loader": "^3.0.2",
    "superstatic": "^6.0.1",
    "text-loader": "0.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.1.1",
    "webpack-cli": "2.1.4",
    "webpack-merge": "4.1.1",
    "workbox-cli": "^3.2.0",
    "worker-loader": "^1.1.1"
  }
}
